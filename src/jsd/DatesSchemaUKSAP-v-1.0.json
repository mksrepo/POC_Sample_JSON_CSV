{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://poc.com/poc/DatesSAP.json",
    "type": "array",
    "items": {
        "$id": "http://poc.com/poc/DatesSAP.json/items",
        "type": "object",
        "properties": {
            "impressionNumber": {
                "$id": "http://poc.com/poc/DatesSAP.json/items/properties/impressionNumber",
                "type": "string",
                "title": "ImpressionNumber",
                "maxLength": 2,
                "default": "",
                "examples": ["AB"]
            },
            "publicationDate": {
                "$id": "http://poc.com/poc/DatesSAP.json/items/properties/publicationDate",
                "type": "string",
                "title": "PublicationDate",
                "maxLength": 8,
                "examples": ["DDMMYYYY"]
            },
            "estimatedStockInDate": {
                "$id": "http://poc.com/poc/DatesSAP.json/items/properties/estimatedStockInDate",
                "type": "string",
                "title": "EstimatedStockInDate",
                "maxLength": 8,
                "default": "",
                "examples": ["DDMMYYYY"]
            },
            "productISBN": {
                "$id": "http://poc.com/poc/DatesSAP.json/items/properties/productISBN",
                "type": "string",
                "title": "Product ISBN",
                "maxLength": 18,
                "default": "",
                "examples": ["ABCD1234"]
            }
        },
        "required": [
            "impressionNumber",
            "productISBN"
        ]
    }
}