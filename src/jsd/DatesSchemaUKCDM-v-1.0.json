{
	 "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://poc.com/poc/DatesCDM.json",
    "type": "object",
    "properties": {
        "impressionNumber": {
            "$id": "/properties/impressionNumber",
            "type": "string",
            "title": "ImpressionNumber",
            "maxLength": 2,
            "default": "",
            "examples": ["AB"]
        },
        "publicationDate": {
            "$id": "/properties/publicationDate",
            "type": "string",
            "title": "PublicationDate",
            "maxLength": 8,
            "examples": ["DDMMYYYY"]
        },
        "estimatedStockInDate": {
            "$id": "/properties/estimatedStockInDate",
            "type": "string",
            "title": "EstimatedStockInDate",
            "maxLength": 8,
            "default": "",
            "examples": ["DDMMYYYY"]
        },
        "productISBN": {
            "$id": "/properties/productISBN",
            "type": "string",
            "title": "Product ISBN",
            "maxLength": 18,
            "default": "",
            "examples": ["ABCD1234"]
        }
    },
    "required": [
        "impressionNumber",
        "productISBN"
    ]
}